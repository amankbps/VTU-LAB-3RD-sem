#include<stdio.h>
#include<ctype.h>
#define MAX 20

char s[MAX];
int top=-1;
void push(char data)
{
    if(top+1==MAX)
    {
        printf("overflow ");
        return;
    }
    s[++top]=data;
}

char pop()
{
    char c=s[top];
    top--;
    return c;
}
int com(char a)
{  if(a=='(')
      return 0;
  else if(a=='+' || a=='-')
        return 1;
  else if(a=='*' || a=='/' || a=='%')
    return 2;
  else if(a=='^')
    return 3;
}
int main()
{   int j=0;
    char infix[MAX];
    char postfix[MAX];
    printf("enter the infix expression: ");
    scanf("%s",infix);
   
    for(int i=0;infix[i];i++)
    {
        if(isalnum(infix[i]))
            postfix[j++]=infix[i];
        else if(infix[i]=='(')
                    push('(');
        else if(infix[i]==')')
        {
            while(s[top]!='(')
            {
                postfix[j++]=pop();


            }
            pop();
        }

        else
        {
            while(com(s[top])>=com(infix[i]))
            {
                postfix[j++]=pop();

            }
            push(infix[i]);
        }

    }
    while(top!=-1)
    {
        postfix[j++]=pop();

    }
  
    printf("%s",postfix);
    return 0;
}
